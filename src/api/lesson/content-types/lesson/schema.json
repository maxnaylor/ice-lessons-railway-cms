    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "lessonPages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lesson-page.lesson-page",
