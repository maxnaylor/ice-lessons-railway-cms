{
  "kind": "collectionType",
  "collectionName": "lesson_pages",
  "info": {
    "singularName": "lesson-page",
    "pluralName": "lesson-pages",
    "displayName": "Lesson Pages",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "vocab",
        "conversation",
        "pronunciation",
        "verb",
        "exercise",
        "summary"
      ],
      "required": true,
      "default": "vocab"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "lesson-page-blocks.paragraph",
        "lesson-page-blocks.vocab-block",
        "lesson-page-blocks.conversation",
        "lesson-page-blocks.callout",
        "lesson-page-blocks.verb",
        "lesson-page-blocks.paradigm"
      ]
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "longTitle": {
      "type": "string"
    },
    "exercise": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::exercise.exercise"
    },
    "lesson": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::lesson.lesson",
      "inversedBy": "lessonPages"
    },
    "pageNo": {
      "type": "integer",
      "unique": false,
      "required": true
    }
  }
}
