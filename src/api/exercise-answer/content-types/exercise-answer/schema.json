{
  "kind": "collectionType",
  "collectionName": "exercise_answers",
  "info": {
    "singularName": "exercise-answer",
    "pluralName": "exercise-answers",
    "displayName": "Exercise Answers"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "exercise": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::exercise.exercise"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "unstarted",
        "started",
        "complete-fullmarks",
        "complete-partmarks"
      ],
      "default": "unstarted"
    },
    "answerJSON": {
      "type": "json"
    },
    "possibleMarks": {
      "type": "integer"
    },
    "correctAnswers": {
      "type": "integer"
    },
    "unfilledAnswers": {
      "type": "integer"
    },
    "lessonPage": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::lesson-page.lesson-page"
    }
  }
}
